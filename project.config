[project]
	description = Autopilot to control FlightGear's aircrafts
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Approximator (approximator)
[access "refs/for/refs/*"]
	read = group fgap
	read = group user/Approximator (approximator)
	push = group fgap
	push = group user/Approximator (approximator)
	label-Code-Review = -2..+2 group fgap
	label-Code-Review = -2..+2 group user/Approximator (approximator)
	label-Verified = -1..+1 group fgap
	label-Verified = -1..+1 group user/Approximator (approximator)
	rebase = group fgap
	rebase = group user/Approximator (approximator)
	removeReviewer = group fgap
	removeReviewer = group user/Approximator (approximator)
	submit = group fgap
	submit = group user/Approximator (approximator)
[access "refs/heads/*"]
	read = group fgap
	read = group user/Approximator (approximator)
	create = group fgap
	create = group user/Approximator (approximator)
	push = group fgap
	push = +force group user/Approximator (approximator)
	pushMerge = group fgap
	pushMerge = group user/Approximator (approximator)
[access "refs/tags/*"]
	push = +force group user/Approximator (approximator)
	pushTag = group user/Approximator (approximator)
	pushSignedTag = group user/Approximator (approximator)
